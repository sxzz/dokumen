{
  "name": "vue-docgen",
  "version": "0.0.0",
  "description": "Vue Component Docs Generator",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --fix --ext .vue,.js,.ts,.jsx,.tsx",
    "build": "tsup && esno scripts/postbuild.ts",
    "test": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@volar/shared": "^0.33.2",
    "@volar/vue-language-service": "^0.33.2",
    "ts-morph": "^14.0.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config-prettier": "^1.7.0",
    "@sxzz/eslint-config-ts": "^1.7.0",
    "@types/node": "*",
    "eslint": "^8.11.0",
    "eslint-define-config": "^1.3.0",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "prettier": "^2.6.0",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "vitest": "^0.7.6",
    "vue": "^3.2.31"
  }
}
